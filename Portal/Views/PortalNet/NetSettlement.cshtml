
@{
    Layout = "~/Views/Shared/_LayoutCase.cshtml";
}


@{
    ViewBag.Title = "NetSettlement";
}

<div class="row">
    <div class="col-md-12">

        <h2>Rozliczenia sieci @ViewBag.Client</h2>

    </div>

</div>


<h2>Wybierz datę:</h2>

@using (Html.BeginForm())
{
    <div class="row">
        <div class="col-md-2">
            @Html.DropDownList("Months", null, new { @class = "form-control"})
        </div>

        <div class="col-md-2 col-md-offset-1">
            @Html.DropDownList("Years", null, new { @class = "form-control" })
        </div>

    </div>

    <br />

    <div class="row">
        <div class="col-md-2">
            <input type="submit" class="btn btn-default"  value="Szukaj" name="search"/>
        </div>

    </div>
}



