@model List<SelectListItem>
@{
    ViewBag.Title = "Account";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <h2>Wybierz firmę</h2>

    </div>
</div>


<div class="row">
    <div class="col-md-12">
        @using (Html.BeginForm("Account", "Home", FormMethod.Post))
        {

        <div class="col-md-2 col-md-offset-3">

            @Html.DropDownList("Accounts", null, new { @class = "form-control" })

        </div>



        <div class="col-md-2">
            <input type="submit" class="btn btn-info" value="Zaloguj" name="logIn" />
        </div>

    }

    </div>
</div>

